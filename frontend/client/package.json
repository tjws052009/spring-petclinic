    "prop-types": "^15.7.2",
    "react": "^15.0.0",
    "react-autosuggest": "^9.4.2",
    "react-datepicker": "^0.64.0",
    "react-dom": "^15.0.0",
    "react-router": "^2.7.0",
    "redbox-react": "^1.2.3",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
