{
  "name": "spring-petclinic-reactjs",
  "version": "0.0.1",
  "description": "Spring Boot Pet Clinic Sample Application with React Frontend and Elastic. Forked from https://github.com/spring-petclinic/spring-petclinic-reactjs",
  "main": "index.js",
  "repository": "https://github.com/elastic/spring-petclinic",
  "scripts": {
    "postinstall": "typings install",
    "install-types": "typings install",
    "test": "jest",
    "test:watch": "jest --watchAll --no-cache",
    "start": "webpack-dev-server --host 0.0.0.0 --port 8081 --disable-host-check --open",
