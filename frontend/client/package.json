    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^1.1.3",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^1.1.11",
